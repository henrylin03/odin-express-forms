<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <link rel="stylesheet" href="/styles/global.css">
</head>
<body>
  <h1><%= title %></h1>
  <ul class="user-list">
    <% if (locals.users) {%>
      <% users.forEach(user => { %>
        <%- include("partials/user", { user: user}) %>
      <% }); %>
    <% } %>
  </ul>
  <a href="/create">Create a user</a>
</body>
</html>
